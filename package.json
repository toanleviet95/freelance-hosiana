{
  "name": "myapp",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ngx-translate-extract -i ./src -m _ --clean --sort --format namespaced-json -o ./src/assets/i18n/vi.json ./src/assets/i18n/en.json ./src/assets/i18n/fr.json",
    "build:ssr": "npm run build && ng run hosiana:server && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "prettier:check": "prettier {src,e2e}/**/*.{ts,json,md,scss} --list-different",
    "prettier": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@ng-bootstrap/ng-bootstrap": "^2.2.2",
    "@ng-select/ng-select": "^2.10.2",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@nguniversal/express-engine": "^6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "axios": "^0.18.0",
    "browserify": "^16.2.3",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "jsencrypt": "^3.0.0-rc.1",
    "ngrx-store-localstorage": "^5.1.0",
    "ngx-autosize": "^1.2.4",
    "ngx-captcha": "^3.1.2",
    "ngx-google-places-autocomplete": "^2.0.1",
    "ngx-permissions": "^5.0.0",
    "ngx-webstorage": "^2.0.1",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "ts-loader": "^4.5.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@angular/platform-server": "^6.1.10",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngrx/store-devtools": "^6.1.0",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "^1.14.3",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-cli": "^3.1.2"
  }
}
